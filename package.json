{
  "name": "element-ui-el-table-draggable",
  "version": "1.2.5",
  "description": "Make 'el-table' of 'element UI' sortable.",
  "main": "dist/element-ui-el-table-draggable.ssr.js",
  "module": "dist/element-ui-el-table-draggable.esm.js",
  "unpkg": "dist/element-ui-el-table-draggable.min.js",
  "browser": {
    "./sfc": "src/SortableElTable.vue"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WakuwakuP/element-ui-el-table-draggable.git"
  },
  "keywords": [
    "element-ui",
    "sortable"
  ],
  "author": "Naoki Fujisawa <naoki.fujisawa@wakuwakup.net> (www.wakuwakup.net)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/WakuwakuP/element-ui-el-table-draggable/issues"
  },
  "homepage": "https://github.com/WakuwakuP/element-ui-el-table-draggable#readme",
  "dependencies": {
    "sortablejs": "^1.9.0"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "minimist": "^1.2.0",
    "rollup": "^1.12.1",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-vue": "^5.0.0",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.6.10"
  }
}
